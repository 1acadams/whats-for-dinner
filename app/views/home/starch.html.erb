<h1>Starch</h1>
<h2>Please type what starches you have in your kitchen. A starch includes pasta, rice and potatoes. </h2>
<% if local_assigns.has_key? :status_msg %>
    <p><%= status_msg %></p>
<% end %>
<%= form_with url: input_starch_path, local: true, method: :post do%>

    <div>
        <%= label_tag "input 1"%>
        <%= text_field_tag "input1", feedback.fetch(:input1, nil) %>
        <br><br>
    </div>
    <div>
        <%= label_tag "input 2"%>
        <%= text_field_tag "input2", feedback.fetch(:input2, nil) %>
        <br><br>
    </div>
    <div>
        <%= label_tag "input 3"%>
        <%= text_field_tag "input3", feedback.fetch(:input3, nil) %>
        <br><br>
    </div>
    <div>
        <%= label_tag "input 4"%>
        <%= text_field_tag "input4", feedback.fetch(:input4, nil) %>
        <br><br>
    </div>
    <div>
        <%= label_tag "input 5"%>
        <%= text_field_tag "input5", feedback.fetch(:input5, nil) %>
    </div>

    <br>
    <%= submit_tag 'Submit'%>
    
    
<% end %>

<p><%= link_to 'Back', index_path%></p>
<p><%= link_to 'Next', veg_path%></p>