<h1>Vegetables</h1>
<h2>Please type what vegetables you have in your kitchen. Fresh vegetables as well as canned and frozen should be included. </h2>
<% if local_assigns.has_key? :status_msg %>
    <p><%= status_msg %></p>
<% end %>
<%= form_with url: input_veg_path, local: true, method: :post do%>

    <div>
        <%= label_tag "input 1"%>
        <%= text_field_tag "input1"%>
        <br><br>
    </div>
    <div>
        <%= label_tag "input 2"%>
        <%= text_field_tag "input2"%>
        <br><br>
    </div>
    <div>
        <%= label_tag "input 3"%>
        <%= text_field_tag "input3"%>
        <br><br>
    </div>
    <div>
        <%= label_tag "input 4"%>
        <%= text_field_tag "input4"%>
        <br><br>
    </div>
    <div>
        <%= label_tag "input 5"%>
        <%= text_field_tag "input5" %>
    </div>
    <div>
    <br>
    <%= submit_tag 'Submit'%>
<% end %>
<p><%= link_to 'Back', protein_path%></p>
<p><%= link_to 'Next', aromatics_path%></p>
<p><%= link_to 'Home', index_path %>
